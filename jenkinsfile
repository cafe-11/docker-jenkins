pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                sh 'printenv'
                sh 'docker build -t dockerpython/jenkinsdemo:"${BUILD_ID}" .'
            }
        }
        stage('Push) {
            steps {
                withDockerRegistry([credentialsId: "docker_creds", url: ""]){
                sh 'docker push dockerpython/jenkinsdemo:"${BUILD_ID}"'
                }
            }
        }
    }
}
            
