pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                sh 'printenv'
                sh 'docker build -t suhas826/jenkinsdemo:"${BUILD_ID}" .'
            }
        }
        stage('Push') {
            steps {
                sh 'docker logout'
                withDockerRegistry([credentialsId: "docker_creds", url: ""]){
                sh 'docker push suhas826/jenkinsdemo:"${BUILD_ID}"'
                }
            }
        }
    }
}
            
